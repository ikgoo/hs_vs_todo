{
    "name": "your-extension",
    "displayName": "Your Extension",
    "description": "Your extension description",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:plaintext",
        "workspaceContains:**/*.hs"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "your-extension.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "your-extension.openHsPreview",
                "title": "Open HS Preview",
                "icon": "$(preview)"
            }
        ],
        "customEditors": [
            {
                "viewType": "hsPreview",
                "displayName": "HS Preview",
                "selector": [
                    {
                        "filenamePattern": "*.hs"
                    }
                ],
                "priority": "option"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceExtname == .hs",
                    "command": "your-extension.openHsPreview",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "build": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "dev": "concurrently \"npm run watch\" \"code .\"",
        "debug": "concurrently \"npm run watch\" \"code --debug\""
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
